<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchResult">

    <View
        android:id="@+id/view1111"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_alignTop="@+id/spinner0101"
        android:layout_marginTop="8dp"
        android:background="#BDBDBD"
        app:layout_constraintTop_toBottomOf="@+id/search22"
        tools:layout_editor_absoluteX="16dp" />

    <View
        android:id="@+id/view0202"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_alignBottom="@+id/spinner0101"
        android:background="#BDBDBD"
        app:layout_constraintTop_toBottomOf="@+id/spinner0202"
        tools:layout_editor_absoluteX="16dp" />

    <Spinner
        android:id="@+id/spinner0101"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view1111" />

    <Spinner
        android:id="@+id/spinner0202"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        app:layout_constraintStart_toEndOf="@+id/spinner0101"
        app:layout_constraintTop_toBottomOf="@+id/view1111" />

    <SearchView
        android:id="@+id/search22"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/listview11"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view0202" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="419dp"
        android:layout_height="641dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/view0202"
        tools:context=".SearchResult">

    <TextView
        android:id="@+id/empty_search"
            android:layout_width="223dp"
            android:layout_height="46dp"
            android:layout_margin="20dp"
        android:text="검색한 결과가 없습니다."
            android:visibility="gone" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>